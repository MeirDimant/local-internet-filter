{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Mitmproxy",
      "type": "debugpy",
      "preLaunchTask": "mitmproxy",
      "request": "attach",
      "connect": {
        "host": "127.0.0.1",
        "port": 5678
      },
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}",
          "remoteRoot": "."
        }
      ]
    }
  ]
}
